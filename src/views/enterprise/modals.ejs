<!-- Modal -->
<% unplastifiedItems.forEach(e=>{ %> 
      <div class="flex justify-center items-center fixed top-0 left-0 z-[1055] hidden w-full h-full overflow-x-hidden overflow-y-auto outline-0 mb-12 <%= `modal-${e.dataValues.unplastified_item_id}`%>">
        <div class="flex flex-col gap-y-4 bg-upfc-gray rounded absolute w-[40%] left-[29%] m-4 p-8 top-0 text-white">
        <div class="modal-header">
          <h5 class="flex gap-x-1 gap-x-1">
            <span>Item Plástico:</span>
            <p class="font-light"><%= e.dataValues.plastic_item %></p>
          </h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <h6 class="flex gap-x-1">
            <label class="form-label" for="implemented_change">Cambio implementado:</label>
            <p class="font-light"><%= e.dataValues.implemented_change %></p>
          </h6>
          <h6 class="flex gap-x-1">
            <label class="form-label" for="sku">SKU:</label>
            <p class="font-light"><%= e.dataValues.sku %></p>
          </h6>
          <h6 class="flex gap-x-1">
            <label class="form-label" for="implemented_change">Fecha de implementación:</label>
            <p class="font-light"><%= e.dataValues.implementation_date %></p>
          </h6>
          <h6 class="flex gap-x-1">
            <label class="form-label" for="implemented_change">Impacto deseado:</label>
            <p class="font-light"><%= e.dataValues.impact_approach.dataValues.impact_approach %></p>
          </h6>
          <h6 class="flex gap-x-1">
            <label class="form-label" for="implemented_change">Unidad de medida:</label>
            <p class="font-light"><%= e.dataValues.id_product_measurement_unit %></p>
          </h6>
          <h6 class="flex gap-x-1">
            <label class="form-label" for="implemented_change">Cantidad:</label>
            <p class="font-light"><%= parseInt(e.dataValues.impact_approach_quantity) %></p>
          </h6>
        </div>
        <%
          const beforePicArray = JSON.parse(e.dataValues.before_pic);
          if (beforePicArray.length > 0) {
        %>
          <h6>Before pics</h6>
          <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
            <% if (beforePicArray.length > 1) { %>
              <ol class="carousel-indicators">
                <% beforePicArray.forEach((pic, index) => { %>
                  <li data-target="#carouselExampleIndicators" data-slide-to="<%= index %>" class="<%= index === 0 ? 'active' : '' %>"></li>
                <% }); %>
              </ol>
            <% } %>
            <div class="carousel-inner">
              <% beforePicArray.forEach((pic, index) => { %>                
                <div class="carousel-item <%= index === 0 ? 'active' : '' %>">
                  <img class="d-block w-100" src="<%= pic %>" alt="<%= pic %> slide">
                </div>
              <% }); %>
            </div>
            <% if (beforePicArray.length > 1) { %>
              <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
              </a>
              <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
              </a>
            <% } %>
          </div>
        <% } %>
        <%
          const afterPicArray = JSON.parse(e.dataValues.after_pic);
          if (afterPicArray.length > 0) {
        %>
          <h6>After pics</h6>
          <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
            <% if (afterPicArray.length > 1) { %>
              <ol class="carousel-indicators">
                <% afterPicArray.forEach((pic, index) => { %>
                  <li data-target="#carouselExampleIndicators" data-slide-to="<%= index %>" class="<%= index === 0 ? 'active' : '' %>"></li>
                <% }); %>
              </ol>
            <% } %>
            <div class="carousel-inner">
              <% afterPicArray.forEach((pic, index) => { %>
                <div class="carousel-item <%= index === 0 ? 'active' : '' %>">
                  <img class="d-block w-100" src= "<%= pic %>" alt="<%= pic %> slide">
                </div>
              <% }); %>
            </div>
            <% if (afterPicArray.length > 1) { %>
              <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
              </a>
              <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
              </a>
            <% } %>
          </div>
        <% } %>
        <%
          const videoArray = JSON.parse(e.dataValues.video);
          if (videoArray.length > 0) {
        %>
          <h6>Videos</h6>
          <div id="videoModal" class="modal fade" tabindex="-1" role="dialog">
            <div class="modal-dialog modal-lg" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h6 class="modal-title">Videos</h6>
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
                <div class="modal-body">
                  <% videoArray.forEach(video => { %>
                    <video class="w-100" controls>
                      <source src="<%= video %>" type="video/mp4">
                      <source src="<%= video %>" type="video/webm">
                      <source src="<%= video %>" type="video/ogg">
                      Your browser does not support the video tag.
                    </video>
                    <br>
                  <% }); %>
                </div>
              </div>
            </div>
          </div>
        <% } %>
        <%
          const technicalFileArray = JSON.parse(e.dataValues.technical_file);
          if (technicalFileArray.length > 0) {
        %>
          <div class="text-center mt-4">
            <button type="button" class="btn btn-primary" id="downloadAllButton">
              Download technical files
            </button>
          </div>
          <% technicalFileArray.forEach(file => { %>
            <a class="d-none" href="<%= file %>" download="<%= file %>" id="downloadLink"></a>
          <% }); %>
        <% } %>
        <%
          const additionalDocs = JSON.parse(e.dataValues.additional_documents);
          if (additionalDocs.length > 0) {
        %>
          <h6>Additional Documents</h6>
          <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
            <% if (additionalDocs.length > 1) { %>
              <ol class="carousel-indicators">
                <% additionalDocs.forEach((doc, index) => { %>
                  <li data-target="#carouselExampleIndicators" data-slide-to="<%= index %>" class="<%= index === 0 ? 'active' : '' %>"></li>
                <% }); %>
              </ol>
            <% } %>
            <div class="carousel-inner">
              <% additionalDocs.forEach((doc, index) => { %>
                <div class="carousel-item <%= index === 0 ? 'active' : '' %>">
                  <img class="d-block w-100" src= "<%= doc %>" alt="<%= doc %> slide">
                </div>
              <% }); %>
            </div>
            <% if (additionalDocs.length > 1) { %>
              <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
              </a>
              <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
              </a>
            <% } %>
          </div>
        <% } %>
       
        <% if (userCategory ==2) { %>
          <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
          </div>
        <% } %>
        <% if (userCategory == 3) { %>
            <div class="modal-footer">
              <form method="POST" action="/unplastify/updateMintingRequestStatus/<%= e.dataValues.minting_request_id %>/In%20Review?_method=PUT">
                <input type="submit" class="btn btn-primary" value="In Review"></input>
              </form>
              <form method="POST" action="/unplastify/updateMintingRequestStatus/ <%= e.dataValues.minting_request_id %>/Approved?_method=PUT">
                <input type="submit" class="btn btn-success" value="Approve"></input>
              </form>
              <form method="POST" action= "<%= `/unplastify/updateMintingRequestStatus/${e.dataValues.minting_request_id}/Further%20documentation%20requested?_method=PUT` %>" >
                <input type="submit" class="btn btn-warning" value="Request further information"></input>
              </form>
              <form method="POST" action="/unplastify/updateMintingRequestStatus/ <%= e.dataValues.minting_request_id %>/Stand-by?_method=PUT">
                <input type="submit" class="btn btn-warning" value="Stand by"></input>
              </form>
              <form method="POST" action="/unplastify/updateMintingRequestStatus/ <%= e.dataValues.minting_request_id %>/'Rejected'?_method=PUT">
                <input type="submit" class="btn btn-danger" value="Decline"></input>
              </form>
            </div>
        <% } %>
        </div>
      </div>
<% }) %>
